// Phase 2 Built-in Test Framework: test blocks, assert

fn add(a, b) {
    return a + b
}

fn clamp(value, min_val, max_val) {
    if value < min_val {
        return min_val
    }
    if value > max_val {
        return max_val
    }
    return value
}

// Assert works outside test blocks too
assert add(1, 2) == 3

